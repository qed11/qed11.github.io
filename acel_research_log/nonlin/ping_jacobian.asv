function ping_jacobian

n = 5;
mu = 0.1;
utrue = fem_nonlin(n, mu, 1e-7);

w 
u = 1 + 0.5*rand(2*n + 1,1);

del = 1e-3;
[~, J] = res(n,mu,u);
Jfd = zeros(2*n + 1,2* n + 1);


for j = 1:n
    u(j) = u(j) + del;
    [r1, ~] = res(n, mu, u);
    u(j) = u(j) - 2*del;
    [r2, ~] = res(n, mu, u);

    
    u(j) = u(j) + del;
    Jfd(:,j) = 0;
end

    norm(J - Jfd, 'fro')

end